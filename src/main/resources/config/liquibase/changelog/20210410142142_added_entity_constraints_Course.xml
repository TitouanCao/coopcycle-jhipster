<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Course.
    -->
    <changeSet id="20210410142142-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="order_id"
                                 baseTableName="course"
                                 constraintName="fk_course_order_id"
                                 referencedColumnNames="id"
                                 referencedTableName="panier"/>

        <addForeignKeyConstraint baseColumnNames="course_id"
                                 baseTableName="course_agents"
                                 constraintName="fk_course_agents_course_id"
                                 referencedColumnNames="id"
                                 referencedTableName="course"/>

        <addForeignKeyConstraint baseColumnNames="agents_id"
                                 baseTableName="course_agents"
                                 constraintName="fk_course_agents_agents_id"
                                 referencedColumnNames="id"
                                 referencedTableName="compte"/>
        
        <addForeignKeyConstraint baseColumnNames="restaurant_id"
                                 baseTableName="course"
                                 constraintName="fk_course_restaurant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="restaurant"/>

    </changeSet>
</databaseChangeLog>
